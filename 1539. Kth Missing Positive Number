class Solution {
public:
    int findKthPositive(vector<int>& arr, int k) {
        vector<int>vec(100000,-1);
        for(int i=0;i<arr.size();i++)
        {
            if(vec[arr[i]])
            {
                vec[arr[i]]=1;
            }
        }
      for(int i=1;i<=100000;i++)
      {
          if(vec[i]==-1)
          {
              k--;
          }
          if(k==0)
          {
              return i;
          }
      }
      return 0;
    }
};
