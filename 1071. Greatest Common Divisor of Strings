class Solution {
public:
      // string gcd(string a,string b)
    // {
    //     if(a.size()==0)
    //     {
    //         return b;
    //     }
    //     if(b.size()==0)
    //     {
    //         return a;
    //     }
    //     if(a.size()<b.size())
    //     {
    //         return gcd(b,a);
    //     }
    //     if(a.substr(0,b.size())!=b)
    //     {
    //         return "";
    //     }
    //     return gcd(a.substr(b.size()),b);
