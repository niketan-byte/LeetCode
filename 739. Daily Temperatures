class Solution {
public:
    vector<int> dailyTemperatures(vector<int>& temp){
      stack<pair<int,int>>s;
      int n = temp.size();
      for(int i=0;i<n;i++)
      {
          int curr = temp[i];
          temp[i]=0;
          while(!s.empty() && curr>s.top().first)
          {
          temp[s.top().second] = i-s.top().second;
          s.pop();
          }
          s.push({curr,i});
      }
      return temp;
    }
};
