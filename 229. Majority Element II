class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {
        int ele1,ele2;
        int cnt1=0;
        int cnt2=0;
        vector<int>ans;
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]==ele1)
            {
              cnt1++;
            }
            else if(nums[i]==ele2)
            {
                cnt2++;
            }
            else if(cnt1==0)
            {
                cnt1=1;
                ele1=nums[i];
            }
            else if(cnt2==0)
            {
                cnt2=1;
                ele2=nums[i];
            }
            else{
                cnt1--;
                cnt2--;
            }
        }

        int ans1=0;
        int ans2=0;
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]==ele1)
            {
                ans1++;
               
            }
            if(nums[i]==ele2)
            {
                ans2++;
               
            }

        }
               if(ans1>nums.size()/3)
                {
                    ans.push_back(ele1);
                }
                 if(ans2>nums.size()/3)
                {
                    ans.push_back(ele2);
                }
        return ans;
    }
};
