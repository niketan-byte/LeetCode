class Solution {
public:
    bool isPathCrossing(string path) {
        vector<pair<int,int>>vis;
        int x=0,y=0;
        vis.push_back({x,y});
        for(auto i:path){
            if(i=='N')y++;
            else if(i=='S')y--;
            else if(i=='E')x++;
            else if(i=='W')x--;
            vis.push_back({x,y});
        }
        set<pair<int,int>>st(vis.begin(),vis.end());

        if(vis.size()==st.size())return false;
        return true;
    }
};
