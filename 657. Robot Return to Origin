class Solution {
public:
    bool judgeCircle(string moves) {
        int u=0,d=0,l=0,r=0;
        for(auto i:moves)
        {
            if(i=='U')
            {
                u +=1;
            }
            if(i=='D')
            {
                d += 1;
            }
            if(i=='L')
            {
                l += 1;
            }
            if(i=='R')
            {
                r += 1;
            }
        }
        if(u==d && l==r)
        {
            return 1;
        }
        return 0;
        
    }
};
