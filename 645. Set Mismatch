class Solution {
public:
    vector<int> findErrorNums(vector<int>& nums) {
        int sum1=accumulate(nums.begin(),nums.end(),0);
        vector<int>ans;
        set<int>s;
        for(auto &i:nums)
        {
            s.insert(i);
        }
        int sum2=accumulate(s.begin(),s.end(),0);
        ans.push_back(sum1-sum2);
        int n=nums.size();
        ans.push_back((n*(n+1))/2-(sum2));
        return ans;
    }
};
