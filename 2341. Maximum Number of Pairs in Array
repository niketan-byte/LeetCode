class Solution {
public:
    vector<int> numberOfPairs(vector<int>& nums) {
         unordered_map<int,int>mp;
         for(int i=0;i<nums.size();i++)
         {
             mp[nums[i]]++;
         }
         int cnt1=0,cnt2=0;
         for(auto i:mp)
         {
             if(i.second%2==0 || i.second/2 >=1)
             {
                  if(i.second%2==0)
                  {
                      cnt1++;
                  }
                  if(i.second%2!=0 && i.second/2>=1)
                  {
                      cnt1 += i.second/2;
                      cnt2 += i.second-(i.second/2)*2;
                  }
             }
         }
         return {cnt1,cnt2};
    }
};
