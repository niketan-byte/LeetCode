class Solution {
public:
    int longestPalindrome(string s) {
    int res=0;
    unordered_map<int,int>mp;
    for(int i=0;i<s.size();i++)
    {
        mp[s[i]]++;
        if(mp[s[i]]%2==0)
        {
            res+=mp[s[i]];
            mp[s[i]]=0;
        }
    }
    for(auto i:mp)
    {
        if(i.second==1)
        {
            res++;
            break;
        }
    }
        return res;
    }
};
