class MyStack {
public:
    queue<int> q1,q2;
    MyStack() {
        
    }
    
    void push(int x) {
        q1.push(x);
    }
    
    int pop() {
        int len = q1.size();
        for(int i=0;i<len-1;i++)
        {
            int temp=q1.front();
            q1.pop();
            q1.push(temp);
        }
        int res = q1.front();
        q1.pop();
        return res;
    }
    
    int top() {
        while(q1.size()>1)
        {
            int x = q1.front();
            q2.push(x);
            q1.pop();
        }
        int x = q1.front();
        q1.pop();
        q2.push(x);
        
        queue<int> temp = q1;
        q1 = q2;
        q2 = temp;
        return x;
    }
    
    bool empty() {
       int s =  q1.size() + q2.size();
        return s == 0;  
    }
};

/**
 * Your MyStack object will be instantiated and called as such:
 * MyStack* obj = new MyStack();
 * obj->push(x);
 * int param_2 = obj->pop();
 * int param_3 = obj->top();
 * bool param_4 = obj->empty();
 */
