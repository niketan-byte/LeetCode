class Solution {
public:
    vector<int> vowelStrings(vector<string>& words, vector<vector<int>>& queries) {
        vector<int>vec;
        for(int i=0;i<words.size();i++)
        {
            string str = words[i];
            
            if((str[0]=='a' || str[0]=='e' || str[0]=='i' || str[0]=='o' || str[0]=='u') && (str[str.size()-1]=='a' || str[str.size()-1]=='e' || str[str.size()-1]=='i' || str[str.size()-1]=='o' || str[str.size()-1]=='u'))
            {
                vec.push_back(1);
            }
            else{
                vec.push_back(0);
            }
            
        }
        for(int i=1;i<vec.size();i++)
        {
            vec[i] += vec[i-1];
        }
        vector<int>ans;
        for(int i=0;i<queries.size();i++)
        {
            int l = queries[i][0];
            int r = queries[i][1];
            if(l==0)
            {
                ans.push_back(vec[r]);
            }
            else{
                ans.push_back(vec[r]-vec[l-1]);
            }
        }
        return ans;
    }
};
