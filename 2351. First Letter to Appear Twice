class Solution {
public:
    char repeatedCharacter(string s) {
        set<char>st;
        for(int i=0;i<s.size();i++)
        {
            auto pos = st.find(s[i]);
            if(pos!=st.end())
            {
                return s[i];
            }
            st.insert(s[i]);
        
        }
        return s[0];
    }
};
