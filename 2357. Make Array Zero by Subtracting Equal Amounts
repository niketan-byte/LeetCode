class Solution {
public:
    int minimumOperations(vector<int>& nums) {
        sort(nums.begin(),nums.end());
        int sum=0;
        int cnt=0;
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]-sum==0)
            {
                continue;
            }
            else{
                sum += (nums[i]-sum);
                cnt++;
            }
        }
return cnt;
    }
};

// 0 1 3 5 5
