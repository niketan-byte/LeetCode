class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int cnt=0;
        int ele;
        for(int i=0;i<nums.size();i++)
        {
            if(cnt==0)
            {
                cnt++;
                ele = nums[i];
            }
            else if(nums[i]==ele)
            {
                cnt++;
            }
            else{
                cnt--;
            }
        }

        // checking
        int ans=0;
        for(int i=0;i<nums.size();i++)
        {
            if(ele==nums[i])
            {
                ans++;
            }
        }
        if(ans>=nums.size()/2)
        {
            return ele;
        }
        return -1;
    }
};
