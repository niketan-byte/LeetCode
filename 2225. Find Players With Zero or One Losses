class Solution {
public:
    vector<vector<int>> findWinners(vector<vector<int>>& matches) {
       map<int,int>mp;
       for(auto m:matches)
       {
           if(!mp[m[0]])
           {
               mp[m[0]]=0;
           }
            mp[m[1]]++;
       }
       vector<int>zero,one;
       for(auto[k,l]:mp)
       {
           if(l==0)
           {
               zero.push_back(k);
           }
           if(l==1)
           {
            one.push_back(k);
           }
       }
       return {zero,one};
    }
};
