class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int curr=0,mx=0;
        for(auto i:nums)
        {
            if(i==1)
            {
                curr++;
                mx = max(mx,curr);
            }
            else{
                curr=0;
            }
        }
        return mx;
    }
};
