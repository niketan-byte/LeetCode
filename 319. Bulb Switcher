class Solution {
public:
    int bulbSwitch(int n) {
      int i=0;
        if(n==0 || n==1)
        {
            return n;
        }
        while(i*i<=n && n>1)
        {
            i++;
        }
        return i-1;  
    }
};

class Solution {
public:
    int bulbSwitch(int n) {
        if(n==0 || n==1)
        {
            return n;
        }
    return sqrt(n);

    }
};
