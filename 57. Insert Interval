class Solution {
public:
    vector<vector<int>> insert(vector<vector<int>>& in, vector<int>& ne) {
        int st=-1;
        int en=-1;

        vector<vector<int>>ans;
      
        for(int i=0;i<in.size();i++)
        {
            if(ne[0]<=in[i][1] && ne[1]>=in[i][0])
            {
                st = min(in[i][0],ne[0]);
                en = max(in[i][1],ne[1]);
                i++;
                while(i<in.size() && en>=in[i][0])
                {

                    en = max(en,in[i][1]);
                    i++;
                }
                i--;
                ans.push_back({st,en});
            }
            else if(st==-1 && in[i][1]>ne[0])
            {
                ans.push_back({ne[0],ne[1]});
                ans.push_back({in[i][0],in[i][1]});
                 st = ne[0];

            }
          else{
              ans.push_back({in[i][0],in[i][1]});
          }
        }
        if(st==-1)
        {
            ans.push_back({ne[0],ne[1]});
        }
return ans;
    }
};
