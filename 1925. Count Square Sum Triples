class Solution {
public:
    int countTriples(int n) {
       vector<bool>square(n*n+1);
        for(int i=1;i<=n;i++)
        {
            square[i*i]=true;
        }
        int res=0;
        for(int i=1;i<=n;i++)
        {
            for(int j=i;i*i+j*j<=n*n;j++)
            {
                res += square[i*i+j*j]*2;
            }
        }
        return res;
    }
};
