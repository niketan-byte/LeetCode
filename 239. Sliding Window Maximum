class Solution {
public:
    vector<int> maxSlidingWindow(vector<int>& nums, int k) {
       multiset<int,greater<int>>st;
       for(int i=0;i<k;i++)
       {
           st.insert(nums[i]);

       }
          vector<int>ans;
          auto it = st.begin();
          ans.push_back(*it);
          st.erase(st.find(nums[0]));

          int j=k;
          int i=1;
          while(j<nums.size())
          {
              st.insert(nums[j]);
          auto it = st.begin();
           
          ans.push_back(*it);
          st.erase(st.find(nums[i]));
          i++;
          j++;
          }


       return ans;

    }
};
