class Solution {
public:
    vector<int> nextGreaterElement(vector<int>& nums1, vector<int>& nums2) {
        map<int,int>mp;
        stack<int>st;
        vector<int>ans(nums1.size(),-1);
        for(int i=0;i<nums2.size();i++)
        {
            int ele = nums2[i];
            while(!st.empty() && st.top()<ele)
            {
                mp[st.top()] = ele;
                st.pop();
            }
            st.push(ele);
        }
        for(int i=0;i<nums1.size();i++)
        {
            int ele=nums1[i];
            if(mp.find(ele)!=mp.end())
            {
                int nge = mp[ele];
                ans[i]=nge;
            }
        }
        
        return ans;
    }
};
