class Solution {
public:
    int differenceOfSum(vector<int>& nums) {
        int sum1 = accumulate(nums.begin(),nums.end(),0);
        int sum2=0;

        for(int i=0;i<nums.size();i++)
        {
            int a = nums[i];
            while(a>0)
            {
                int rem = a%10;
                sum2 += rem;
                a/=10;
            }
        }

        return abs(sum1-sum2);
    }
};
