class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
      set<int>s(nums1.begin(),nums1.end());
      // set<int>s1;
      vector<int>ans;
      for(auto i:nums2)
      {
          // if(s.find(i)!=s.end())
          // {
          //     s1.insert(i);
          // }
          if(s.erase(i))
          {
              ans.push_back(i);
          }
      }
        // for(auto i:s1)
        // {
        //     ans.push_back(i);
        // }
        return ans;
    }
};
