class Solution {
public:
    int computeArea(int ax1, int ay1, int ax2, int ay2, int bx1, int by1, int bx2, int by2) {
        int uoa = abs((ax2-ax1)*(ay2-ay1)) + abs((bx2-bx1)*(by2-by1));
        int a=max(ax1,bx1);

        int b=max(ay1,by1); 
  
        int c=min(ax2,bx2);  
    
        int d=min(ay2,by2);
        if(c>a && d>b)
        {
 int oa = abs((c-a)*(d-b));
  return abs(uoa-oa);
        }
       
        return uoa;
       
    }
};
