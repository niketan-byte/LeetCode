class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int k=nums.size();
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]==val)
            {
                nums[i]=-1;
                k--;
            }
        }
        int i=0;
        for(auto n:nums)
        {
            if(n!=-1)
            {
                nums[i++]=n;
            }
        }
        // while()
        // sort(nums.begin(),nums.end());
        // for(int i=0;i<k;i++)
        // {
        //     cout<<nums[i]<<" ";
        // }
        return k;
    }
};
