class Solution {
public:
    vector<int> findThePrefixCommonArray(vector<int>& A, vector<int>& B) {
      int n = A.size();
      vector<int>seen(n+1);
      vector<int>vec(n);
      int curr=0;
      for(int i=0;i<n;i++)
      {
          if(++seen[A[i]]==2)
          {
              curr++;
          }
          if(++seen[B[i]]==2)
          {
              curr++;
          }
          
          vec[i]=curr;
      }
      return vec;  
    }
};
