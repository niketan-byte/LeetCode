class Solution {
public:
    string categorizeBox(int length, int width, int height, int mass) {
        long long int a1 = length;
        long long int a2 = width;
        long long int a3 = height;

       long long int v = a1*a2*a3;
        int b=0;
        int h=0;
        if(length>=10000 || width>=10000 || height>=10000 || v>=1e9)
        {
            b=1;
        }
        if(mass>=100)
        {
            h=1;
        }
        if(b==1 && h==1)
        {
            return "Both";
        }
         if(b!=1 && h!=1)
        {
            return "Neither";
        }
         if(b==1 && h!=1)
        {
            return "Bulky";
        }
         if(b!=1 && h==1)
        {
            return "Heavy";
        }
        return "Heavy";
    }
};
