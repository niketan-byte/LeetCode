class Solution {
public:
    int minImpossibleOR(vector<int>& nums) {
       set<int>s;
        for(int i:nums)
        {
            s.insert(i);
        }
        long long int ans=1;
        while(1)
        {
            if(s.find(ans)==s.end())
            {
                return ans;
            }
            ans *= 2;
        }
        return -1;
    }
};
