class Solution {
public:
    int maximumUniqueSubarray(vector<int>& nums) {
    set<int> s;
    int i=0,j=0;
    int sum=0,maxx=0;
    while(j<nums.size())
    {
        // cout<<i<<" "<<j<<" sum "<<sum<<endl;
        sum += nums[j];
        
        // repeated
        if(s.find(nums[j])!=s.end())
        {
            // delete so that all element unique
            while(i<j && nums[i]!=nums[j])
            {
                sum -= nums[i];
                s.erase(nums[i++]);
            }
            sum -= nums[i];
            i+=1;
            j+=1;
        }
        else
        {
            s.insert(nums[j]);
            maxx = max(maxx,sum);
            j++;
        }
    }
        return maxx;
    }
};
