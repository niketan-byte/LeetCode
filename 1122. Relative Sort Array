class Solution {
public:
    vector<int> relativeSortArray(vector<int>& arr1, vector<int>& arr2) {
        map<int,int>mp;
        for(auto i:arr1)
        {
            mp[i]++;
        }
        vector<int>ans;

        for(int i=0;i<arr2.size();i++)
        {
         if(mp.find(arr2[i])!=mp.end())
         {
             while(mp[arr2[i]]--)
             {
                 ans.push_back(arr2[i]);
             }
             mp.erase(arr2[i]);
         }  
        }
        for(auto i:mp)
        {
            int count = i.second;
            int ele = i.first;
            while(count--)
            {
                ans.push_back(ele);
            }
        }
        return ans;
    }
};
