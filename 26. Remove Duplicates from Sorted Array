class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
    //   int cnt=1;
    //   for(int i=0;i<nums.size()-1;i++)
    //   {
    //       if(nums[i]==nums[i+1])
    //       {
    //       nums[i]=101;
    //       }
    //       else{
    //           cnt++;
    //       }
    //   }
    //   sort(nums.begin(),nums.end());


    int cnt=0;
    for(int i=1;i<nums.size();i++)
    {
        if(nums[i]==nums[i-1])
        {
            cnt++;
        }
        else{
            nums[i-cnt]=nums[i];
        }
    }

      return nums.size()-cnt;  
    }
};
