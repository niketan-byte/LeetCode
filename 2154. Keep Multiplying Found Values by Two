class Solution {
public:
    int findFinalValue(vector<int>& nums, int original) {
       map<int,bool>mp;
        for(int i=0;i<nums.size();i++)
        {
            mp[nums[i]]=true;
        }
        if(mp[original])
        {
            while(mp[original])
            {
                original *= 2;
            }
        }
        return original;
    }
};
