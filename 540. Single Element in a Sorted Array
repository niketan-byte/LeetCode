class Solution {
public:
    int singleNonDuplicate(vector<int>& nums) {
        int n=nums.size();
        int l=0;
        int r=n-1;
        int mid;
        while(l<r)
        {
            mid = l+(r-l)/2;
            int newmid = mid%2==0?mid+1:mid-1;
            if(nums[mid]==nums[newmid])
            {
                 l=mid+1;
            }
            else{
                r = mid;
            }
        }
        return nums[l];

    }
};
