class Solution {
public:
    int partitionString(string s) {
        int ans=0;
        set<int>st;
        for(int i=0;i<s.size();i++)
        {
            // auto pos = st.find(s[i]-'0');
            if(st.find(s[i]-'0')!=st.end())
            {
                ans++;
                st.clear();
                st.insert(s[i]-'0');
                cout<<ans<<" "<<i<<endl;
            }
            else{
                st.insert(s[i]-'0');
            }
        }
        return ans+1;
    }
};
