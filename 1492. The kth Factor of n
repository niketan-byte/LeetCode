class Solution {
public:
    int kthFactor(int n, int k) {
       vector<int>vec(n+1,0);

       for(int i=1;i<vec.size();i++)
       {
          if(n%i==0)
          {
              vec[i] = i;
              k--;
          }
          if(k==0)
          {
              return i;
          }
       }
       return -1;
    }
};
