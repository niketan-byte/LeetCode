class Solution {
public:
    vector<vector<int>> findMatrix(vector<int>& nums) {
        unordered_map<int,int>mp;
        for(int i:nums)
        {
            mp[i]++;
        }
        vector<vector<int>>ans;
        while(mp.size())
        {
            vector<int>v;
            for(auto i:mp)
            {
                v.push_back(i.first);
            }
            for(int i:v)
            {
                if(!--mp[i])
                {
                    mp.erase(i);
                }
            }
            ans.push_back(v);
        }
        return ans;
    }
};
