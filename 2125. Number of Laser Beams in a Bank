class Solution {
public:
   
       int numberOfBeams(vector<string>& bank) {
    int prev = 0, res = 0;
    for (auto &r : bank) {
        int cur = count(begin(r), end(r), '1');
        if (cur) {
            res += prev * cur;
            prev = cur;
        }
    }
    return res;

    }
};

class Solution {
public:
    int numberOfBeams(vector<string>& bank) {
        int res=0,prev=0;
        for(int i=0;i<bank.size();i++){
            int cnt=0;
            for(int j=0;j<bank[i].size();j++)
            {
                if(bank[i][j]=='1')
                {
                    cnt++;
                }
            }
            if(cnt){
                res+=cnt*prev;
                prev=cnt;
            }

        }
        return res;
    }
};
