class Solution {
public:
    int countBalls(int lowLimit, int highLimit) {
       vector<int>count(46);
        for(int i=lowLimit;i<=highLimit;i++)
        {
            int temp = i;
            int sum = 0;
            while(temp!=0)
            {
                sum += temp%10;
                temp = temp/10;
            }
            ++count[sum];
        }
        return *max_element(begin(count),end(count));
    }
};
