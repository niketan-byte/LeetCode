class Solution {
public:
    bool uniqueOccurrences(vector<int>& arr) {
        map<int,int>mp;
        for(int i=0;i<arr.size();i++)
        {
          mp[arr[i]]++;
        }
        set<int>st;
        for(auto i:mp)
        {
            if(st.count(i.second))
            {
                return false;
            }
            else{
                st.insert(i.second);
            }
        }
        return true;
    }
};
