class Solution {
public:
    int getLucky(string s, int k) {
        string a="";
          int n=0;
          int sum=0;
        for(int i=0;i<s.size();i++)
        {
            n = s[i]-'a'+1;
            sum += n/10+n%10;
        }
int sum1=0;
    //   cout<<sum;
       while(--k>0 && sum>9)
       {
           while(sum)
           {sum1 += sum%10;
           sum=sum/10;
           }
           swap(sum,sum1);
       }
        return sum;
    }
};
