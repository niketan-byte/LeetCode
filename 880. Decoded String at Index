class Solution {
public:
    string decodeAtIndex(string s, int k) {
        string res="";
        long long len = 0;
        for(auto ch:s)
        {
            if(isdigit(ch))
            {
                len *= ch-'0';
            }
            else{
                len++;
            }
        }

        for(int i=s.size()-1;i>=0;i--)
        {
            if(isdigit(s[i]))
            {
                len /= (s[i]-'0');
                k = k%len;
            }
            else{
                if(k==0 || len==k)
                {
                    return string("")+s[i];
                }
                len--;
            }
        }
        return "";
    }
};
