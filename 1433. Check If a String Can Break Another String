class Solution {
public:
    bool checkIfCanBreak(string s1, string s2) {
       sort(s1.begin(),s1.end());
       sort(s2.begin(),s2.end());
       bool l1=0;
       for(int i=0;i<s1.size();i++)
       {
           if(s1[i]>=s2[i])
           {
             l1=1;
           }
           else{
               l1=0;
               break;
           }
       } 
       if(l1)
       {
           return true;
       }
       bool l2=0;
       for(int j=0;j<s2.size();j++)
       {
           if(s2[j]>=s1[j])
           {
               l2=1;
           }
           else{
               l2=0;
               break;
           }
       }
       if(l2)
       {
           return true;
       }
return false;
    }
};
