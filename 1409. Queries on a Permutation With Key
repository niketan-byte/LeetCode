class Solution {
public:
    vector<int> processQueries(vector<int>& queries, int m) {
        vector<int>P(m+1);
        for(int i=0;i<m;i++)
{
    P[i+1] =i;
}
        for(int i=0;i<queries.size();i++)
        {
         int tmp = P[queries[i]];
         for(int j=1;j<=m;j++)
         {
             if(P[j]<tmp)
             {
                 P[j]++;
             }
         }
         P[queries[i]]=0;
         queries[i] = tmp;
        }
        return queries;
    }
};
