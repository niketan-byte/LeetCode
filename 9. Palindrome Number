class Solution {
public:
    bool isPalindrome(int x) {
        string s = to_string(x);
        int j=s.length();
        for(int i=0;i<s.length()/2;i++)
        {
            if(s[i]!=s[j-1])
            {
                return false;
            }
            j--;
        }
        return true;
    }
};
