class Solution {
public:
    int findSpecialInteger(vector<int>& arr) {
        map<int,int>mp;
        for(auto i:arr)
        {
            mp[i]++;
        }
        int n = arr.size()/4;
        for(auto i:mp)
        {
            if(i.second>n)
            {
                return i.first;
            }
        }
        return 0;
    }
};
