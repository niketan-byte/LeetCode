class Solution {
public:
    vector<vector<int>> findDifference(vector<int>& nums1, vector<int>& nums2) {
     unordered_set<int>s1;
     unordered_set<int>s2;
     vector<int>res;
     vector<vector<int>> ans;   
     for(int i=0;i<nums1.size();i++)
      {
        s1.insert(nums1[i]);
      }
     for(int i=0;i<nums2.size();i++)
      {
        s2.insert(nums2[i]);
      }
      for(auto i:s1)
      {
          if(s2.find(i)==s2.end())
          {
              res.push_back(i);
          }
      }
        ans.push_back(res);
        res.clear();
        
    for(auto i:s2)
    {
        if(s1.find(i)==s1.end())
        {
            res.push_back(i);
        }
    }
        ans.push_back(res);
        return ans;
    }
};


class Solution {
public:
    vector<vector<int>> findDifference(vector<int>& nums1, vector<int>& nums2) {
        vector<vector<int>>vec;
        set<int>s1,s2;
      for(auto i:nums1)
      {
          s1.insert(i);
      }
      for(auto i:nums2)
      {
          s2.insert(i);
      }
       vector<int>ans;
        set_difference(begin(s1), end(s1),  
                 begin(s2), end(s2),  
                 inserter(ans, end(ans)));  
      vec.push_back(ans);

      
      vector<int>ans2;
        set_difference(begin(s2), end(s2),  
                 begin(s1), end(s1),  
                 inserter(ans2, end(ans2)));  
     
      vec.push_back(ans2);
      return vec;
    }
};
