class Solution {
public:
    vector<vector<int>> findDifference(vector<int>& nums1, vector<int>& nums2) {
     unordered_set<int>s1;
     unordered_set<int>s2;
     vector<int>res;
     vector<vector<int>> ans;   
     for(int i=0;i<nums1.size();i++)
      {
        s1.insert(nums1[i]);
      }
     for(int i=0;i<nums2.size();i++)
      {
        s2.insert(nums2[i]);
      }
      for(auto i:s1)
      {
          if(s2.find(i)==s2.end())
          {
              res.push_back(i);
          }
      }
        ans.push_back(res);
        res.clear();
        
    for(auto i:s2)
    {
        if(s1.find(i)==s1.end())
        {
            res.push_back(i);
        }
    }
        ans.push_back(res);
        return ans;
    }
};
