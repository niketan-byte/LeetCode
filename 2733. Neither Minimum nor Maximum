class Solution {
public:
    int findNonMinOrMax(vector<int>& nums) {
       int mn = *min_element(nums.begin(),nums.end());
       int mx = *max_element(nums.begin(),nums.end());
       int ans=-1;
       for(auto i:nums)
       {
           if(i!=mn && i!=mx)
           {
               ans = i;
               return ans;
           }
       }
       return ans; 
    }
};
