class Solution {
public:
    vector<int> twoOutOfThree(vector<int>& nums1, vector<int>& nums2, vector<int>& nums3) {
        map<int,int>mp;
        set<int>s1,s2,s3;
        for(auto i:nums1)
        {
            s1.insert(i);
        }
        for(auto i:nums2)
        {
            s2.insert(i);
        }
        for(auto i:nums3)
        {
            s3.insert(i);
        }
        vector<int>ans;
        for(auto i:s1)
        {
            mp[i]++;
        }
        for(auto i:s2)
        {
            mp[i]++;
        }
        for(auto i:s3)
        {
            mp[i]++;
        }
        for(auto i:mp)
        {
            if(i.second>=2)
            {
                ans.push_back(i.first);
            }
        }
        return ans;
    }
};
