class Solution {
public:
    int maxProfit(vector<int>& prices) {
       int cooldown=0,sell=0,buy=-prices[0];
        for(int i=0;i<prices.size();i++)
        {
            int prevsell=sell;
            sell=buy+prices[i];
            buy=max(buy,cooldown-prices[i]);
            cooldown = max(cooldown,prevsell);
        }
         return max(sell, cooldown);
    }
};
