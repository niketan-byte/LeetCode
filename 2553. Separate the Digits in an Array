class Solution {
public:
    vector<int> separateDigits(vector<int>& nums) {
        vector<int>ans;
        for(int i=0;i<nums.size();i++)
        { 
            vector<int>res;
            while(nums[i]>0)
            {
                int rem = nums[i]%10;
                nums[i] = nums[i]/10;
                res.push_back(rem);
            }
            ans.insert(ans.begin(),res.begin(),res.end());
        }
        reverse(ans.begin(),ans.end());
        return ans;
    }
};
