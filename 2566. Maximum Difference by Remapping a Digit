class Solution {
public:
    int minMaxDifference(int num) {
       string str = to_string(num);
        int high,low;
        for(int i=0;i<str.size();i++)
        {
            if(str[i]!='9')
            {
                high = str[i]-'0';
                break;
            }
            
        }
         for(int i=0;i<str.size();i++)
        {
            if(str[i]!='0')
            {
                low = str[i]-'0';
                break;
            } 
        }
        string str2 = str;
        for(int i=0;i<str.size();i++)
        {
            if(str[i]-'0'==high)
            {
                str[i]='9';
            }
        }
        for(int i=0;i<str2.size();i++)
        {
             if(str2[i]-'0'==low)
            {
                str2[i]='0';
            }
        }
        int ans = stoi(str)-stoi(str2);
        return ans;
    }
};
