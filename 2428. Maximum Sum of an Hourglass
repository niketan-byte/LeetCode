class Solution {
public:
    int maxSum(vector<vector<int>>& grid) {
        int m = grid.size();
        int n = grid[0].size();
        int mx=0;
        for(int i=0;i<m-2;i++)
        {
           for(int j=0;j<n-2;j++)
           {
               int sum=0;
              
                  for(int c=j;c<j+3;c++)
           {
                 sum += grid[i][c];
                 sum+= grid[i+2][c];
           }
           sum += grid[i+1][j+1];
             mx  = max(mx,sum);   
           }
                            
           }
    
    
        
return mx;

            }
};
