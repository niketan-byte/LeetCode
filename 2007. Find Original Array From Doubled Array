class Solution {
public:
       vector<int> findOriginalArray(vector<int>& changed) {
       sort(changed.begin(),changed.end());
           	int n = changed.size();
           if(n & 1) return {};
           map<int,int>mp;
       for(int i=0;i<changed.size();i++)
       {
           mp[changed[i]]++;
       }
           vector<int>ans;
           for(int i=0;i<changed.size();i++)
           {
        int twice = changed[i]*2;
		if(mp[changed[i]]==0) continue;

		if(mp[twice]==0) 
			return {};

		mp[twice]--;
		mp[changed[i]]--; 
		ans.push_back(changed[i]);
           }
           return ans;
    }
};
