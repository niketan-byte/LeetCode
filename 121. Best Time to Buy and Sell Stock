class Solution {
public:
    int maxProfit(vector<int>& prices) {
       int n = prices.size();
        int res=INT_MIN;
        int ans=prices[0];
        for(int i=0;i<n;i++)
        {
           
                res = max(res,prices[i]-ans);
                ans = min(ans,prices[i]);
            }
        
        return res;
    }
};
