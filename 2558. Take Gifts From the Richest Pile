class Solution {
public:
    long long pickGifts(vector<int>& gifts, int k) {
       while(k--)
       {
    auto it = std::minmax_element(gifts.begin(), gifts.end());
    int max_idx = std::distance(gifts.begin(), it.second);
           
          gifts[max_idx] = floor(sqrt(gifts[max_idx]));
       }
      long long int sum=0;
        for(auto i:gifts)
        {
            sum += i;
        }
        return sum;
        }
};
