class Solution {
public:
    double average(vector<int>& salary) {
     long long int sum = accumulate(salary.begin(),salary.end(),0);
     int mn = *min_element(salary.begin(),salary.end());
     int mx = *max_element(salary.begin(),salary.end());
     cout<<mn<<mx<<sum;
     return (sum-mn-mx)*1.0/(salary.size()-2);
    }
};
