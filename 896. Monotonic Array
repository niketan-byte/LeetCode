class Solution {
public:
    bool isMonotonic(vector<int>& nums) {
        int cnt1=1,cnt2=1;
        for(int i=0;i<nums.size()-1;i++)
        {
            if(nums[i]<=nums[i+1])
            {
                cnt1++;
            }
            if(nums[i]>=nums[i+1]){
                cnt2++;
            }
        }
        
        if(cnt1==nums.size() || cnt2==nums.size())
        {
            return true;
        }
        cout<<cnt1<<" "<<cnt2;
    return false;
    }
};
