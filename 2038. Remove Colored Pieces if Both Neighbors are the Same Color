class Solution {
public:
    bool winnerOfGame(string colors) {
        int alice=0,bob=0;
        int A=0,B=0;
        for(int i=0;i<colors.length();i++)
        {
          if(colors[i]=='A')
          {
              A++;
              bob += max(0,B-2);
              B=0;
          }
          if(colors[i]=='B')
          {
              B++;
              alice += max(0,A-2);
              A=0;
          }
          
        }
        alice += max(0,A-2);
        bob += max(0,B-2);


        if(alice==0)
        {
            return false;
        }
        if(alice>bob)
        {
            return true;
        }
        return false;
    }
};
